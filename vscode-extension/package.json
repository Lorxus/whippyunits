{
  "name": "whippyunits-refactor",
  "displayName": "WhippyUnits Refactor",
  "description": "Generate type aliases from unit! macro expressions",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:rust"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "whippyunits.generateUnitAlias",
        "title": "Generate Unit Alias",
        "category": "WhippyUnits"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "whippyunits.generateUnitAlias",
          "when": "editorLangId == rust",
          "group": "refactor"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.26.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}
